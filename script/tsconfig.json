{
  "compilerOptions": {
    "target": "ES6",
    "module": "ESNext",
    "strict": true,
    "importHelpers": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "removeComments": true,
    "sourceMap": true,
    "baseUrl": ".",
    "paths": {},
    "lib": ["es6"],
    "alwaysStrict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "types": ["google-apps-script"]
  },
  "include": ["**/*.ts", "../types/**/*.ts"],
  "exclude": ["node_modules"]
}
